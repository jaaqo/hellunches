{
  "name": "hellunches",
  "version": "0.1.0",
  "dependencies": {
    "@lingui/react": "2.9.1",
    "next": "9.5.3",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "build": "next build",
    "lint": "npx eslint src",
    "prettier": "npx prettier \"**/*.+(js|jsx|ts|tsx|md|css|less|scss|json|yml|yaml)\"",
    "format": "npm run prettier -- --write",
    "validate": "npm run lint && npm run prettier -- --list-different",
    "i18n:add": "lingui add-locale",
    "i18n:extract": "lingui extract",
    "i18n:compile": "lingui compile"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@lingui/cli": "2.9.1",
    "@lingui/macro": "2.9.1",
    "@types/node": "14.10.1",
    "@types/react": "16.9.49",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-react": "7.20.6",
    "prettier": "2.1.1",
    "typescript": "4.0.2"
  }
}
